// Test lambdas with non-conflicting names
mydouble = fn x -> x * 2
println 'Double of 5:'
mydouble 5 | println

mysum = fn a, b -> a + b
println 'Sum of 3 and 4:'
mysum 3, 4 | println

mymultiply = fn x, y, z -> x * y * z
println 'Multiply 2, 3, 4:'
mymultiply 2, 3, 4 | println

println 'All lambda tests passed!'
