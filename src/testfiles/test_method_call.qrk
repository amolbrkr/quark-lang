// Test method calls with arguments: obj.method arg

// --- List methods ---
nums = [1, 2, 3]
nums.push 4
nums.push 5
println nums.length

// get by index
println (nums.get 0)
println (nums.get 4)

// insert at index
nums.insert 0, 99
println (nums.get 0)

// set at index
nums.set 0, 100
println (nums.get 0)

// remove at index
nums.remove 0
println nums.length

// slice
s = nums.slice 0, 2
println s.length

// --- String methods ---
msg = 'hello world'
println (msg.contains 'world')
println (msg.contains 'xyz')
println (msg.startswith 'hello')
println (msg.endswith 'world')
println (msg.replace 'world', 'quark')
println (msg.concat '!')

// --- Chaining with methods ---
data = [5, 4, 3, 2, 1]
data.reverse
println data.length

// --- Error cases ---
x = 42
x.push 5
