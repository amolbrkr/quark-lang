// Test module system

// Define a math module
module math:
    fn square n ->
        n * n

    fn cube n ->
        n * n * n

// Use the math module
use math

// Test module functions
println 'Square of 5:'
square 5 | println

println 'Cube of 3:'
cube 3 | println

// Test built-in functions
println 'Length of hello:'
len 'hello' | println

println 'Convert int to string:'
str 42 | println

println 'Convert string to int:'
int '123' | println
