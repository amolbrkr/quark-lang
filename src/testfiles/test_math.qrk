// Test math module functions

println 'Testing Math Module'
println '==================='

// abs - absolute value
println 'abs(-5):'
neg5 = 0 - 5
abs neg5 | println

println 'abs(3.14):'
abs 3.14 | println

// min and max
println 'min(10, 5):'
min 10, 5 | println

println 'max(10, 5):'
max 10, 5 | println

println 'min(3.5, 2.1):'
min 3.5, 2.1 | println

// sqrt
println 'sqrt(16):'
sqrt 16 | println

println 'sqrt(2):'
sqrt 2 | println

// floor, ceil, round
println 'floor(3.7):'
floor 3.7 | println

println 'ceil(3.2):'
ceil 3.2 | println

println 'round(3.5):'
round 3.5 | println

println 'round(3.4):'
round 3.4 | println

// Chained operations
println 'sqrt(abs(-16)):'
neg16 = 0 - 16
neg16 | abs | sqrt | println

println 'floor(sqrt(10)):'
sqrt 10 | floor | println
