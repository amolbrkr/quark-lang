{
  "name": "quark-language",
  "displayName": "Quark Language Support",
  "description": "Syntax highlighting for the Quark language",
  "version": "0.0.2",
  "publisher": "quark-lang",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "quark",
        "aliases": ["Quark", "quark"],
        "extensions": [".qrk"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "quark",
        "scopeName": "source.quark",
        "path": "./syntaxes/quark.tmLanguage.json"
      }
    ]
  }
}