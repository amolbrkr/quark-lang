fn add x: int, y: int -> x + y

fn head xs: list[int] -> get xs, 0

fn tail xs: list[int] -> get xs, -1

fn join a: str, b: str -> a + b

fn is_even n: int -> (n % 2) == 0

double = fn x: int -> x * 2

